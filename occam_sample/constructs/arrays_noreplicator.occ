SEQ
    [2][3]INT slots:
    [3]CHAN OF INT chans:
    SEQ
        PAR
            chans[0] ! 0
            chans[1] ! 1
            chans[2] ! 2
            chans[0] ? slots[1][0]
            chans[1] ? slots[0][1]
            chans[2] ? slots[0][2]
        SEQ
            SERIAL ! slots[1][0]
            SERIAL ! slots[0][1]
            SERIAL ! slots[0][2]